#include <iostream>
#include <math.h>
using namespace std;
int main(){
	int A, A2, A3, A4, A5, A10, A15;
	cin>>A;
	A2=A*A;
	A3=A2*A;
	A5=A3*A2;
	A10=A5*A5;
	A15=A10*A5;
	cout<<"A2="<<A2<<endl;
	cout<<"A3="<<A3<<endl;
	cout<<"A5="<<A5<<endl;
	cout<<"A10="<<A10<<endl;
	cout<<"A15="<<A15<<endl;
	return 0;
}
