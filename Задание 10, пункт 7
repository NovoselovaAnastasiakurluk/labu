#include <iostream>
#include <math.h>
using namespace std;
int main(){
	int a, b, c, x, y, z;
	cout<<"a= ";
	cin>>a;
	cout<<"b= ";
	cin>>b;
	cout<<"c= ";
	cin>>c;
	if((a+b>c)&(a+c>b)&(b+c>a)){
		cout<<"Treygolnic syshestvyet"<<endl;
	}
	else{
		cout<<"Treygolnic ne syshestvyet"<<endl;
	}
	return 0;
}
