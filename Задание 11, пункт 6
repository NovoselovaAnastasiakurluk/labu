#include <iostream>
#include <math.h>
using namespace std;
int main(){
	int n;
	cout<<"n= ";
	cin>>n;
	if((n>0)&(n<1000)){
		if(n==0){
		    cout<<"nulevoye chislo"<<endl;
	    }
	    if((n>0)&(n<100)&(n%2==0)){
	    	cout<<"chetnoye dvuznachnoye chislo"<<endl;
       	}
    	if((n>0)&(n<100)&(n%2!=0)){
	    	cout<<"ne chetnoye dvuznachnoye chislo"<<endl;
    	}
       if((n>99)&(n<1000)&(n%2==0)){
	    	cout<<"chetnoye trekhznachnoye chislo"<<endl;
       	}
    	if((n>99)&(n<1000)&(n%2!=0)){
	    	cout<<"ne chetnoye trekhznachnoye chislo"<<endl;
    	}
	}
	return 0;
}
