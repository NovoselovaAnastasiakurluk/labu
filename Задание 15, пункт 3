#include <iostream>
#include <math.h>
using namespace std;
float RingS(float R1, float R2)
{
	return 3.14*(R1*R1-R2*R2);
}
int main()
{
	float R1, R2;
	int i;
	for(i=0; i<3; i++){
		cout<<"R1 = ";
        cin>>R1;
        cout<<"R2 = ";
        cin>>R2;
        cout<<RingS(R1, R2)<<endl;
	}
	return 0;
}
